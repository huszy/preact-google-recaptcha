  "author": "Huszak Gabor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/huszy/preact-google-recaptcha"
  },
  "bugs": {
    "url": "https://github.com/huszy/preact-google-recaptcha/issues"
  },
  "homepage": "https://github.com/huszy/preact-google-recaptcha",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
