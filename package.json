    "es6-shim": "^0.35.3",
    "eslint": "^4.7.2",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-jest": "^21.1.0",
    "eslint-plugin-react": "^7.4.0",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.3",
    "path": "^0.12.7",
    "phantomjs": "^2.1.7",
    "preact": "^8.2.5",
